{
    "name": "@gltf-transform/core",
    "version": "3.0.0-alpha.4",
    "repository": "github:donmccurdy/glTF-Transform",
    "homepage": "https://gltf-transform.donmccurdy.com/",
    "description": "glTF 2.0 SDK for JavaScript and TypeScript, on Web and Node.js.",
    "author": "Don McCurdy <dm@donmccurdy.com>",
    "license": "MIT",
    "type": "module",
    "main": "./dist/core.cjs",
    "module": "./dist/core.modern.js",
    "exports": "./dist/core.modern.js",
    "source": "./src/core.ts",
    "types": "./dist/core.d.ts",
    "sideEffects": false,
    "browserslist": [
        "defaults",
        "not IE 11",
        "node >= 14"
    ],
    "scripts": {
        "dist": "microbundle --format modern,cjs --define PACKAGE_VERSION=$npm_package_version",
        "watch": "microbundle watch --format modern,cjs --define PACKAGE_VERSION=$npm_package_version",
        "watch:debug": "microbundle watch --no-compress --format modern,cjs --define PACKAGE_VERSION=$npm_package_version"
    },
    "keywords": [
        "gltf",
        "3d",
        "model",
        "webgl",
        "threejs"
    ],
    "files": [
        "dist/",
        "src/",
        "README.md",
        "package.json",
        "package-lock.json"
    ],
    "browser": {
        "fs": false,
        "path": false
    },
    "dependencies": {
        "property-graph": "^1.0.0"
    },
    "mangle": {
        "regex": "^_"
    },
    "gitHead": "381ba367afd6179de695be1e4a771b337ccdabad"
}
